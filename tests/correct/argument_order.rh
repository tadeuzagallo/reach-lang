//RUN: %reach | %check

function inspect(%T: Type, x: T) -> Void
{
    print(x.stringify())
    print(" : ")
    println(T.stringify())
}

function g(x: Number, y: Number, z: Number) -> Number {
    inspect([x, y, z]) // CHECK-L: [7, 13, 42] : Number[]
    x
}
function f(_: String, x: Number, y: Number, z: Number) -> Number {
    g(x, y, z)
}

f("", 7, 13, 42) // CHECK: 7 : Number
