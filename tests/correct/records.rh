// RUN: %reach | %check

let x = { y = 42 };
inspect(x) // CHECK: {y = 42} : {y: Number}
inspect(x.y) // CHECK: 42 : Number

function f() -> Number { 13 }
let x = { x = f };
inspect((x.x)()) // CHECK: 13 : Number
inspect(x.x.()) // CHECK: 13 : Number

function g() -> { x: Number } {
    { x = 7 }
}
inspect(g().x) // CHECK: 7 : Number

function h() -> {:} {
    {}
}

inspect(h()) // CHECK: {} : {:}
